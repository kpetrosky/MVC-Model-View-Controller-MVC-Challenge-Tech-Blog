{{#if loggedIn}}
<section class="blog_entries">
    {{!-- console.log(err); --}}
    <div class="blog_entry">
        <h2>{{description}}</h2>
        <p>{{blog_entry}}</p>
    </div>

    <div class="row mt-4">
        <div class="col-md-6">
            <form class="form new-project-form">
                <div class="form-group">
                    <label for="blog_name">Blog Entries</label>
                    <input class="form-input" type="text" id="blog-entry" name="blog_entry" />
                </div>
            </form>
        </div>
    </div>
</section>
{{else}}
<a href="/login">You must login first to view this page</a>
{{/if}}
<script src="/public/js/"> </script>